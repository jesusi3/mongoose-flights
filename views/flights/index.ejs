<%-  include("../partials/header") %>

<h1>Flights List</h1>
<table>
    <thead>
        <tr>
            <th>Airline</th>
            <th>Airport </th>
            <th>FlightNo</th>
            <th>Departure</th>
        </tr>
    </thead>
    <tbody>
    <%  flight.forEach(function(f) {  %>
        <tr>
            <td><%= f.airline %></td>
            <td><%= f.airport %></td>
            <td><%= f.flightNo %></td>
            <td><%= f.departs %></td>
            <td><a href="/flights/<%= f.id %>">Details</a></td>
        </tr>
    <% }) %>
    </tbody>
</table>

<%-  include("../partials/footer")%>